{
  "name": "crwn-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server":"node server.js",
    "json:server": "json-server --watch data.json",
    "app": "concurrently --kill-others-on-fail \"yarn json:server\" \"yarn server\" \"yarn client\"",
    "start": "npm run app",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm start"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.9.0",
    "firebase-admin": "^8.10.0",
    "graphql": "^15.0.0",
    "http-proxy-middleware": "^1.0.3",
    "json-server": "^0.16.1",
    "socket.io": "^2.3.0",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.19.1"
  }
}
